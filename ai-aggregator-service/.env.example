# Server Configuration
AGG_SERVER_HOST=0.0.0.0
AGG_SERVER_PORT=8080

# Database Configuration
AGG_DATABASE_HOST=localhost
AGG_DATABASE_PORT=5432
AGG_DATABASE_USER=postgres
AGG_DATABASE_PASSWORD=postgres
AGG_DATABASE_DBNAME=ai_aggregator
AGG_DATABASE_SSLMODE=disable

# Redis Configuration
AGG_REDIS_HOST=localhost
AGG_REDIS_PORT=6379
AGG_REDIS_PASSWORD=
AGG_REDIS_DB=0

# NATS Configuration
AGG_NATS_URL=nats://localhost:4222

# Logging Configuration
AGG_LOGGING_LEVEL=info
